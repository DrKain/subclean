{
    "name": "subclean",
    "version": "1.2.2",
    "description": "A CLI package to clean subtitle files of advertising",
    "main": "lib/index.js",
    "scripts": {
        "build": "tsc && pkg . && ts-node postbuild.ts"
    },
    "keywords": [
        "subtitle",
        "clean",
        "advertising",
        "cli",
        "bazarr"
    ],
    "author": "Kain (ksir.pw)",
    "license": "MIT",
    "dependencies": {
        "minimist": "^1.2.5",
        "subtitle": "^4.0.1",
        "update-notifier": "^5.1.0"
    },
    "devDependencies": {
        "fs-extra": "^9.1.0"
    },
    "pkg": {
        "out-path": "./bin/",
        "assets": [
            "filters/**/*",
            "package.json"
        ]
    },
    "bin": {
        "subclean": "lib/index.js"
    }
